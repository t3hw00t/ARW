{
  "$schema": "https://t3hw00t.github.io/ARW/spec/schemas/runtime_adapter_manifest.schema.json",
  "id": "sample.warn.adapter",
  "version": "0.1.0",
  "name": "Sample Warn Adapter",
  "description": "Example with non-fatal warnings for demonstration.",
  "modalities": ["text"],
  "tags": ["Bad Tag", "bad_tag", "bad_tag"],
  "entrypoint": {
    "crate_name": "warn_adapter",
    "symbol": "create_adapter"
  },
  "resources": {
    "accelerator": "cpu",
    "recommended_memory_mb": 256,
    "recommended_cpu_threads": 8,
    "requires_network": true
  },
  "consent": {
    "summary": "Connects to network services"
  },
  "metrics": [
    {"name": "tokens_processed_total"},
    {"name": "tokens_processed_total"}
  ],
  "health": {
    "poll_interval_ms": 500,
    "grace_period_ms": 600
  }
}
